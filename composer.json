{
    "name": "olivernorden/silverstripe-sail",
    "description": "Silverstripe docker",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "docker"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "OliverNorden\\SilverstripeSail\\": "src/",
            "OliverNorden\\SilverstripeSail\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/sail"],
    "minimum-stability": "dev",
    "prefer-stable": true
}